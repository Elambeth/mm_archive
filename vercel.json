{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      },
      {
        "src": "frontend/build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "main.py"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/build/$1"
      }
    ],
    "functions": {
      "main.py": {
        "maxDuration": 60
      }
    }
  }